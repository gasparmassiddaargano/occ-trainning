    "@oracle-cx-commerce/react-components": "3.8.0",
    "@oracle-cx-commerce/react-widgets": "3.8.0",
    "@oracle-cx-commerce/styles": "3.8.0",
    "@oracle-cx-commerce/subscribers": "3.8.0",
    "@oracle-cx-commerce/commerce-utils": "3.8.0",
    "@oracle-cx-commerce/resources": "3.8.0"
  },
  "devDependencies": {
    "@oracle-cx-commerce/rollup-config": "3.8.0"
  },
  "occ": {
